        "lerna": "^8.1.7",
        "playwright": "^1.46.0",
        "postcss": "8.4.41",
        "postcss-preset-env": "^10.0.0",
        "prettier": "^3.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-native": "0.73.7",
        "rollup": "^4.19.1",
        "rollup-plugin-copy": "^3.5.0",
