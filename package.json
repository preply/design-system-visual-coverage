{
    "name": "@preply/ds-workspace",
    "version": "0.0.0",
    "private": true,
    "workspaces": {
        "packages": [
            "packages/*",
            "support/*",
            "themes/*"
        ]
    },
    "engines": {
        "node": "20.12.2",
        "yarn": "3"
    },
    "scripts": {
        "dev": "run build:partials && lerna run dev --parallel --ignore @preply/ds-docs",
        "test": "lerna run test --parallel --no-bail",
        "lint": "run lint:json && eslint --report-unused-disable-directives .",
        "clean": "lerna run clean --parallel --stream --no-bail",
        "build": "run clean && run build:partials && run build:docs"
    },
    "devDependencies": {
        "@babel/core": "7.25.2",
        "@babel/plugin-transform-runtime": "7.24.7",
